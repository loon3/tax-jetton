# TON Wallet Private Key (24-word mnemonic or hex private key)
# You can generate one using:
# 1. TON Wallet app or 
# 2. The script provided in this project
PRIVATE_KEY="your_24_word_mnemonic_phrase_here"

# Alternative: Use hex private key instead of mnemonic
# PRIVATE_KEY_HEX="your_hex_private_key_here"

# Jetton Metadata URL (optional - can be updated later)
JETTON_METADATA_URL="https://example.com/tax-jetton-metadata.json"

# Jetton Info
JETTON_NAME="Tax Jetton"
JETTON_SYMBOL="TAX"
JETTON_DESCRIPTION="A jetton with 10% transfer tax to developer"
JETTON_DECIMALS=9
